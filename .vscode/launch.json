{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "调试本地进程（-gcflags=\"all=-N -l\"）",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 12302
        },
        {
            "name": "注入命名空间调试",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["--cmd=java","--pid=14835","--p0=threaddump"]
        },
        {
            "name": "独立运行收集器",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-web.listen-address",":9172"]
        },
        {
            "name": "集成运行收集器",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["--runtime.gomaxprocs=2"]
        },
        {
            "name": "Beat调试器",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/beats/oneagent",
            "args": ["--web.config.file","","--web.listen-address",":9999","--collector.softirqs", "--collector.systemd", "--collector.systemd.unit-include","(iptables|httpd|sshd).service" ],
        },
        {
            "name": "FileBeat调试器",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/beats/filebeat"
        },
        {
            "name": "debug enter Namespace(c++)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/setNamespace.out",
            "args": ["18868" ,"threaddump"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "buildNamespace"
        },
    ]
}